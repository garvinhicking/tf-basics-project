{
    "name": "sbuerk/tf-basics-project",
    "description": "TYPO3 Demo Project to demonstrate basic testing integration",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "Stefan BÃ¼rk",
            "email": "stefan@buerk.tech"
        }
    ],
    "require": {
        "internal/custom-command": "@dev",
        "internal/custom-middleware": "@dev",
        "typo3/cms-adminpanel": "^13.1",
        "typo3/cms-backend": "^13.1",
        "typo3/cms-belog": "^13.1",
        "typo3/cms-beuser": "^13.1",
        "typo3/cms-core": "^13.1",
        "typo3/cms-dashboard": "^13.1",
        "typo3/cms-extbase": "^13.1",
        "typo3/cms-extensionmanager": "^13.1",
        "typo3/cms-filelist": "^13.1",
        "typo3/cms-fluid": "^13.1",
        "typo3/cms-fluid-styled-content": "^13.1",
        "typo3/cms-form": "^13.1",
        "typo3/cms-frontend": "^13.1",
        "typo3/cms-info": "^13.1",
        "typo3/cms-install": "^13.1",
        "typo3/cms-lowlevel": "^13.1",
        "typo3/cms-opendocs": "^13.1",
        "typo3/cms-recycler": "^13.1",
        "typo3/cms-redirects": "^13.1",
        "typo3/cms-reports": "^13.1",
        "typo3/cms-rte-ckeditor": "^13.1",
        "typo3/cms-scheduler": "^13.1",
        "typo3/cms-seo": "^13.1",
        "typo3/cms-setup": "^13.1",
        "typo3/cms-styleguide": "^13.1",
        "typo3/cms-tstemplate": "^13.1",
        "typo3/cms-viewpage": "^13.1",
        "typo3/cms-workspaces": "^13.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "garvinhicking/typo3-tf-runtests": "^0.2.1",
        "typo3/testing-framework": "^8.0.9",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.11"
    },
    "repositories": {
        "extensions": {
            "type": "path",
            "url": "packages/*"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SBUERK\\TfBasicsProject\\Tests\\": "Tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
